extends template

block pageVars
	- var pageTitle = "Tag Inputs"
	- var pageSubTitle = "Diffrent elements and styles of Tag Inputs."
	- var description  = "jQuery plugin providing a Twitter Bootstrap user interface for managing tags"
	- var activeMenu = "form-tags-input"



block breadcrumbs
	+breadcrumbs([{url:"index.html",name:"Home"},{url:"#",name:"Form Components"}, {name:"Tag Inputs"}])



//- Include page content in page block
append content
	include ../contents/form-tags-input.html

append vendorcss
	link(rel='stylesheet', type='text/css', href=app_assets_path + '/vendors/css/ui/prism.min.css')
	link(rel='stylesheet', type='text/css', href=app_assets_path + '/vendors/css/forms/tags/tagging.css')
	//- link(rel='stylesheet', type='text/css', href=app_assets_path + '/vendors/css/forms/tags/bootstrap-tagsinput.css')

append vendorjs
	//- Bootstrap tagsinput
	//- script(src=app_assets_path + '/vendors/js/forms/tags/bootstrap-tagsinput.min.js', type='text/javascript')
	//- script(src=app_assets_path + '/vendors/js/forms/extended/typeahead/typeahead.bundle.min.js', type='text/javascript')
	script(src=app_assets_path + '/vendors/js/forms/tags/tagging.min.js', type='text/javascript')
	script(src=app_assets_path + '/vendors/js/ui/prism.min.js', type='text/javascript')

append pagejs
	//-  Template JS
	script(src=app_assets_path + '/js/scripts/forms/tags/tagging.js', type='text/javascript')