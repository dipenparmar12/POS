extends template

block pageVars
	- var pageTitle    = "Semi Dark Layout"
	- var pageSubTitle = "Semi dark template layout"
	- var description  = "The semi dark layout has semi dark version navbar, dark navigation & dark footer."
	- var activeMenu   = "semi-dark"



append pageConfig
	- var headerType = 'sk-header'
	- var logoType = 'light'
	- var starterKit  = true

append navbarConfig
	- var navbarColor = "navbar-semi-dark"
	- var navbarBrandCenter = false

block breadcrumbs
	+breadcrumbs([{url:"index.html",name:"Home"},{url:"#",name:"Starters Kit"}, {name:"Semi Dark Layout"}])



//- Include page content in page block
append content
	include ../../contents/starter-kit/starter-page-content.html